i = int(input("Upto what number do you want primes?"))

    
if i ==2:
    print(2)
    
    
if i == 3 or 4:
    print(2)
    print(3)
    
    

if i >4:
    print(2)
    print(3)
    prime = [2,3]
    for n in range(5,i+1):
        for p in prime:
            if p<=n**(1/2):
                if n%p == 0:
                    break
        else:
            print(n)
            prime.append(n)
            
